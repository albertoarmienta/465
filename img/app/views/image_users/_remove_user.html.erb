<%= form_for([@thisImage, @newUser], method: :destroy) do |f| %>
  <% if @newUser.errors.any? %>
  <div id="error_explanation">
  <h3><%= pluralize(@newUser.errors.count, "error") %> did not allow for this user to be added</h3>
  <ul>
    <% @newUser.errors.full_messages.each do |text| %>
      <li><%= text %><li>
    <% end %>
  </ul>
</div>
  <% end %>


<div >
  <%= f.select(:user_id, @usersWhoHaveAccessContents) %>
</div>
<br>

<div class="button">
  <%= f.submit "Remove User"%>
</div>
<% end %>

